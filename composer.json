{
  "name": "bunnyholding/emm-theme-hoo",
  "type": "emm-theme",
  "description": "EMMTheme Hoo",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "oomphinc/composer-installers-extender": "^1.1",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "extra": {
    "installer-name": "Hoo"
  },
  "autoload": {
    "psr-4": {
      "EMMWeb\\EMMTheme\\Hoo\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "EMMWeb\\EMMTheme\\Hoo\\Tests\\": "/Tests/"
    }
  }
}
